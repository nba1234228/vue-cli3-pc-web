{
    "name": "vue-cli3-pc-web",
    "version": "1.0.0",
    "description": "vue-cli3-pc-web pc端前端模版",
    "main": "index.js",
    "scripts": {
        "copy:dev": "node tool/copy.js ./env/dev.js ./src/core/env.js",
        "copy:mock": "node tool/copy.js ./env/mock.js ./src/core/mock.js",
        "copy:test": "node tool/copy.js ./env/test.js ./src/core/test.js",
        "copy:prod": "node tool/copy.js ./env/prod.js ./src/core/prod.js",
        "lint": "eslint --ext .js,.vue src",
        "lint-fix": "eslint --fix --ext .js,.jsx,.vue src/",
        "test": "karma start test/karma.conf.js",
        "vuex:generate": "node tool/vuex_generate.js ./src/vuex ./src/generate/store.js",
        "router:generate": "node tool/router_generate.js ./src/pages ./src/generate/route.js",
        "server:dev": "npm run vuex:generate && npm run router:generate && npm run copy:dev && cross-env NODE_ENV=development webpack-dev-server --config vue.config.js",
        "server:mock": "npm run vuex:generate && npm run router:generate && npm run copy:mock && cross-env NODE_ENV=development webpack-dev-server --config vue.config.js",
        "server:test": "npm run vuex:generate && npm run router:generate && npm run copy:test && cross-env NODE_ENV=development webpack-dev-server --config vue.config.js",
        "server:prod": "npm run vuex:generate && npm run router:generate && npm run copy:prod && cross-env NODE_ENV=development webpack-dev-server --config vue.config.js",
        "build:test": "npm run vuex:generate && npm run router:generate && npm run copy:test && cross-env NODE_ENV=production webpack --config vue.config.js",
        "build:prod": "npm run vuex:generate && npm run router:generate && npm run copy:prod && cross-env NODE_ENV=production webpack --config vue.config.js"
    },
    "author": "fage",
    "license": "ISC",
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "devDependencies": {
        "autoprefixer": "^9.7.5",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.24.1",
        "chai": "^4.2.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.2",
        "css-loader": "^3.4.2",
        "es6-promise": "^4.2.8",
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "eslint-plugin-vue": "^6.2.2",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.0.1",
        "karma": "^4.4.1",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-cli": "^2.0.0",
        "karma-coverage": "^2.0.1",
        "karma-mocha": "^1.3.0",
        "karma-webpack": "^4.0.2",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^7.1.1",
        "node-sass": "^4.13.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-url": "^8.0.0",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "url-loader": "^4.0.0",
        "vue-loader": "^15.9.1",
        "vue-template-compiler": "^2.6.11",
        "watchify": "^3.11.1",
        "webpack": "^4.42.1",
        "webpack-bundle-analyzer": "^3.6.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "element-ui": "^2.13.0",
        "vue": "^2.6.11",
        "vue-i18n": "^8.15.6",
        "vue-router": "^3.1.6",
        "vuex": "^3.1.3"
    }
}
